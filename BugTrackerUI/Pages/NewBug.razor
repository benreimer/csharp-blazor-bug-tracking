
@page "/new-bug"

<h3>Add New Bug</h3>


<EditForm Model="AddBug">

    <InputText id="title" @bind-Value="AddBug.Title" />
    <InputText id="description" @bind-Value="AddBug.Description" />
    <InputNumber id="inputnumber" @bind-Value="AddBug.Priority" />
    <button type="submit">Add Bug</button>

</EditForm>
@code {
    public Bug AddBug = new Bug();
}


